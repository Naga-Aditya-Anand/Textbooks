<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#217aff"
    android:id="@+id/fragment_conatainer_mqp"
    tools:context=".X.XMQP">

    <com.ismaeldivita.chipnavigation.ChipNavigationBar
        android:id="@+id/bottom_nav_menu_mqp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:cnb_menuResource="@menu/qpaper_nav_bar"
        android:layout_alignParentBottom="true"
        android:background="@drawable/round_bar"
        app:cnb_unselectedColor="#000"
        app:cnb_radius="8dp"
        app:cnb_orientationMode="horizontal"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:layout_marginTop="30dp"
        android:layout_marginBottom="30dp"
        android:elevation="8dp"
        />

</RelativeLayout>